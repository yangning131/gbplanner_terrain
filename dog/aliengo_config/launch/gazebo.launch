

<!-- 仿真更改地方 搜索==================== STATE ESTIMATION ==================== bringup去掉了odomtf 即包括了odometry -->

<launch>
    <arg name="robot_name"         default="/"/> <!-- Change this for namespacing. -->
    <arg name="rviz"               default="false"/> <!-- Set to true to run rviz in parallel. -->
    <arg name="lite"               default="false" />  <!-- Set to true if you're using CHAMP lite version. Only useful for microcontrollers. -->
    <arg name="ros_control_file"   default="$(find aliengo_config)/config/ros_control/ros_control.yaml" /> <!-- Path to ROS Control configurations. Do not touch. -->
    <!-- <arg name="gazebo_world"       default="$(find aliengo_config)/worlds/terrain_world3.world" />  -->
    <arg name="gazebo_world"       default="$(find aliengo_config)/worlds/terrain_world45.world" /> 

    <!-- terrain_world3.world 凹陷地形     -->
    <!--mywords23(走廊扩大后)   generated_world（走廊）door_room.world（探索进门扩大）  Path to Gazebo world you want to load. -->
    <!-- <arg name="gazebo_world"       default="$(find aliengo_config)/worlds/terrain/map1.world" />  -->

    <!-- <arg name="world_init_x"       default="24.5" />     15.5   31.5    -25.5  8X Initial position of the robot in Gazebo World  (find aliengo_config)/worlds/default.world"  generated_world -->
    <!-- <arg name="world_init_y"       default="7.5" />        5.5  4Y Initial position of the robot in Gazebo World -->
    <!-- <arg name="world_init_heading" default="-1.57" />     0Initial heading of the robot in Gazebo World -->

    <!-- <arg name="world_init_x"       default="0" />     
    <arg name="world_init_y"       default="0" />        
    <arg name="world_init_heading" default="0" /> -->
     <!-- 0Initial heading of the robot in Gazebo World -->


     <!-- 凹陷地形中狗的位置 -->
     <arg name="world_init_x"       default="-2.0" />     
     <arg name="world_init_y"       default="-2.0" />        
     <arg name="world_init_heading" default="0" />

     <!-- <arg name="world_init_x"       default="-15.0" />     
     <arg name="world_init_y"       default="10.0" />        
     <arg name="world_init_heading" default="0" /> -->


    <param name="use_sim_time" value="true" />

    <include file="$(find aliengo_config)/launch/bringup.launch">
        <arg name="robot_name"             value="$(arg robot_name)"/>
        <arg name="gazebo"                 value="true"/>
        <arg name="lite"                   value="$(arg lite)"/>
        <arg name="rviz"                   value="$(arg rviz)"/>
        <arg name="joint_controller_topic" value="joint_group_position_controller/command"/>
        <arg name="hardware_connected"     value="false"/>
        <arg name="publish_foot_contacts"  value="false"/>
        <arg name="close_loop_odom"        value="true"/>
    </include>

    <include file="$(find champ_gazebo)/launch/gazebo.launch">
        <arg name="robot_name"         value="$(arg robot_name)"/>
        <arg name="lite"               value="$(arg lite)"/>
        <arg name="ros_control_file"   value="$(arg ros_control_file)"/>
        <arg name="gazebo_world"       value="$(arg gazebo_world)"/>
        <arg name="world_init_x"       value="$(arg world_init_x)" />
        <arg name="world_init_y"       value="$(arg world_init_y)" />
        <arg name="world_init_heading" value="$(arg world_init_heading)" />
    </include>    
</launch>
